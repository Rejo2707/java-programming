import java.io.*;
import java.util.*;
import java.text.*;
import java.math.*;
import java.util.regex.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
            int i = in.nextInt();
            int j = in.nextInt();
            char c = in.next().charAt(0);
            float add;
            float sub;
            float mul;
            float div;
            float mod;
            switch(c){
                    case'+':{
                        add = i + j;
                        System.out.printf("Addition of two number is %.1f",add);
                        break;
                    }
                    case'-':{
                        sub = i - j;
                        System.out.printf("Subtraction of two number is %.1f",sub);
                        break;
                    }
                    case'*':{
                        mul = i * j;
                        System.out.printf("Multiplication of two number is %.1f",mul);
                        break;
                    }
                    case'/':{
                        div = i / j;
                        System.out.printf("Division of two number is %.1f",div);
                        break;
                    }
                    case'%':{
                        mod = i % j;
                        System.out.printf("Modulo of two number is %.1f",mod);
                        break;
                    }
                default:{
                    System.out.println("Invalid Input");
                }
                    
                    
            }
        }
    }
