import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
            int n = in.nextInt();
            int count=0;
            int arr[]=new int[n];
            for(int i =0;i<n;i++){
                arr[i]=in.nextInt();
                count = count + arr[i];
            }
            float avg =(float)count/n;
            System.out.printf("Array Mean Value is %.2f",avg);
        
    }
}
