import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
       Scanner in = new Scanner(System.in);
        String s = in.nextLine().toLowerCase();
        int r = 0;
        for(int i =0;i<s.length();i++){
            char c =s.charAt(i);
            if(c=='a' || c=='e' || c=='i' || c=='o' || c=='u'){
                r++;
                 
               
            }
        
        }
        System.out.println(r);
    }
}
    
