import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
        Scanner in = new Scanner(System.in);
          char a = 'A';
        int n = in.nextInt();
        if(n<=9){
        for(int i=0;i<n;i++){
          
            for(int j=0;j<=i;j++){
                System.out.print(a+" ");
                 a++;
            }
            System.out.println();
           
        }
        
        
    }
        else{
            System.out.println("Invalid Input");
        }
}}
