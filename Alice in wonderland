import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
    Scanner in = new Scanner(System.in);
        int n = in.nextInt();
        int j = n%10;
        int k = n/10;
        int i = j+k;
        System.out.println("Bird said:"+n);
        System.out.println("Alice must go in path-"+i);
    }
}
