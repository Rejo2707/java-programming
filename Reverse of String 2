import java.io.*;
import java.util.*;

public class Solution {

    public static void main(String[] args) {
      Scanner in = new Scanner(System.in);
        String s = in.nextLine();
        int n = s.length();
        for (int i=n-1;i>=0;i--){
            
            System.out.print(s.charAt(i));
        }
    }
}
